{
    "swagger": "2.0",
    "info": {
        "title": "Education website with Swagger",
        "description": "Getting started with Laravel and Swagger",
        "termsOfService": "",
        "contact": {
            "email": "name@example.com"
        },
        "version": "1.0.0"
    },
    "host": "localhost:8000",
    "basePath": "/",
    "schemes": [
        "http"
    ],
    "paths": {
        "api/users/login/": {
            "post": {
                "description": "Return a user's information",
                "parameters": [
                    {
                        "name": "email",
                        "in": "query",
                        "description": "Your email",
                        "required": true,
                        "type": "string"
                    },
                    {
                        "name": "password",
                        "in": "query",
                        "description": "Your password(length>=8)",
                        "required": true,
                        "type": "string"
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Successfully",
                        "schema": {
                            "properties": {
                                "access_token": {
                                    "type": "string"
                                },
                                "token_type": {
                                    "type": "string"
                                },
                                "expires_in": {
                                    "type": "integer"
                                },
                                "id": {
                                    "type": "integer"
                                },
                                "email": {
                                    "type": "string"
                                },
                                "email_verified_at": {
                                    "type": "datetime"
                                },
                                "status": {
                                    "type": "string"
                                },
                                "created_at": {
                                    "type": "timestamp"
                                },
                                "updated_at": {
                                    "type": "timestamp"
                                }
                            }
                        }
                    },
                    "401": {
                        "description": "Unauthorized!"
                    }
                }
            }
        },
        "api/users/checkDomain/": {
            "post": {
                "description": "Return a company's information",
                "parameters": [
                    {
                        "name": "domain",
                        "in": "query",
                        "description": "Company Domain",
                        "required": true,
                        "type": "string"
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Successfully",
                        "schema": {
                            "properties": {
                                "name": {
                                    "type": "string"
                                },
                                "domain": {
                                    "type": "string"
                                },
                                "size": {
                                    "type": "integer"
                                },
                                "created_at": {
                                    "type": "datetime"
                                },
                                "updated_at": {
                                    "type": "datetime"
                                }
                            }
                        }
                    },
                    "422": {
                        "description": "No one have domain!"
                    }
                }
            }
        },
        "api/users/logout/": {
            "post": {
                "description": "Return a user's information",
                "responses": {
                    "200": {
                        "description": "User successfully signed out"
                    },
                    "401": {
                        "description": "No one login!"
                    }
                },
                "security": [
                    {
                        "api_key_security_example": []
                    }
                ]
            }
        },
        "api/users/refresh/": {
            "post": {
                "description": "Return a user's information",
                "responses": {
                    "200": {
                        "description": "Successfully",
                        "schema": {
                            "properties": {
                                "access_token": {
                                    "type": "string"
                                },
                                "token_type": {
                                    "type": "string"
                                },
                                "expires_in": {
                                    "type": "integer"
                                },
                                "id": {
                                    "type": "integer"
                                },
                                "email": {
                                    "type": "string"
                                },
                                "email_verified_at": {
                                    "type": "datetime"
                                },
                                "status": {
                                    "type": "string"
                                },
                                "created_at": {
                                    "type": "timestamp"
                                },
                                "updated_at": {
                                    "type": "timestamp"
                                }
                            }
                        }
                    },
                    "401": {
                        "description": "Unauthorized!"
                    }
                },
                "security": [
                    {
                        "api_key_security_example": []
                    }
                ]
            }
        },
        "api/users/register/": {
            "post": {
                "description": "Return a account's information",
                "parameters": [
                    {
                        "name": "code",
                        "in": "query",
                        "description": "6 char",
                        "required": true,
                        "type": "string"
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Successfully",
                        "schema": {
                            "properties": {
                                "email": {
                                    "type": "string"
                                },
                                "role": {
                                    "type": "integer"
                                },
                                "password": {
                                    "type": "string"
                                },
                                "status": {
                                    "type": "string"
                                },
                                "created_at": {
                                    "type": "datetime"
                                },
                                "updated_at": {
                                    "type": "datetime"
                                }
                            }
                        }
                    },
                    "422": {
                        "description": "No one have code"
                    }
                }
            }
        },
        "api/users/getCode/": {
            "post": {
                "description": "Return a user's information",
                "parameters": [
                    {
                        "name": "first_name",
                        "in": "query",
                        "description": "max 255 char",
                        "required": true,
                        "type": "string"
                    },
                    {
                        "name": "last_name",
                        "in": "query",
                        "description": "max 255 char",
                        "required": true,
                        "type": "string"
                    },
                    {
                        "name": "email",
                        "in": "query",
                        "description": "email",
                        "required": true,
                        "type": "string"
                    },
                    {
                        "name": "password",
                        "in": "query",
                        "description": "min 8 char",
                        "required": true,
                        "type": "string"
                    },
                    {
                        "name": "domain",
                        "in": "query",
                        "description": "max 255 char",
                        "required": true,
                        "type": "string"
                    },
                    {
                        "name": "size",
                        "in": "query",
                        "required": true,
                        "type": "integer"
                    },
                    {
                        "name": "company_name",
                        "in": "query",
                        "description": "max 255 char",
                        "required": true,
                        "type": "string"
                    },
                    {
                        "name": "role",
                        "in": "query",
                        "required": true,
                        "type": "integer"
                    },
                    {
                        "name": "over_view",
                        "in": "query",
                        "required": true,
                        "type": "string"
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Successfully. Please check code your email!"
                    },
                    "401": {
                        "description": "Missing Data"
                    }
                }
            }
        }
    },
    "definitions": {}
}